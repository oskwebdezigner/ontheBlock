{
  "expo": {
    "name": "Hawta",
    "slug": "hawta",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
     
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0",
      "config": {
        "googleMapsApiKey": "AIzaSyD0vSz1qjsn_RSPBB9HRD1Eqztm7DyuKw8"
      },
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses the location to determine the delivery address for your orders."
      },
      "bundleIdentifier": "com.app.hawta"
    },
    "android": {
      "versionCode": 0,
      "package":  "com.app.hawta",
      
      "useNextNotificationsApi": true,
      "permissions":["CAMERA","LOCATION","ACCESS_FINE_LOCATION"]
    },
    "notification": {
      "iosDisplayInForeground": true
