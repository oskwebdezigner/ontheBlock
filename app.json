{
  "expo": {
    "name": "OntheBlock",
    "slug": "ontheblock",
    "version": "0.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.3",
      "config": {
        "googleMapsApiKey": "AIzaSyD0vSz1qjsn_RSPBB9HRD1Eqztm7DyuKw8"
      },
      "usesAppleSignIn": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses the location to determine the delivery address for your orders."
      },
      "bundleIdentifier": "com.app.ontheblock"
    },
    "android": {
      "versionCode": 0,
      "package": "com.app.ontheblock",
      "useNextNotificationsApi": true,
      "permissions": ["CAMERA", "LOCATION", "ACCESS_FINE_LOCATION"]
    },
    "googleServicesFile": "./google-services.json",
    "notification": {
      "iosDisplayInForeground": true
    },
    "plugins": ["expo-media-library"]
  }
}
